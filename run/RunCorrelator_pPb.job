Universe = vanilla
Executable = /atlasgpfs01/usatlas/workarea/jeff/JetHadronCorrelations/bin/RunCorrelator.exe
Priority = +1
GetEnv = True
InitialDir = /atlasgpfs01/usatlas/workarea/jeff/JetHadronCorrelations
accounting_group = group_atlas.boulder


# Useful variables to set:
# Analysis-level minimum/maximum jet pT; 15, 30, 60, or 120 (minimum); 0 (maximum)
#minjpt=30
maxjpt=0


# Input subdirectory; 50GeVJetsData, 100GeVJetsData, or MinBiasData (at the moment)
#sigdir=MinBiasData

# Input subdirectory for mixing pool; MinBiasData (at the moment)
mixdir=MinBiasData

# Variation subdirectory; Nominal, JetES5PercUpVar, JetES5PercDownVar, JetES5PercSmearVar, JetES2PercUpVar, JetES2PercDownVar, JetES2PercSmearVar, FcalCentVar (at the moment)
#vardir=JetES5PercSmearVar

# Centrality selection
#cent=0


# p+Pb non-mixed
Arguments = $(minjpt) $(maxjpt) pPb16_5TeV $(minjpt)GeVJets_data16 ./rootFiles/Histograms/$(minjpt)GeVJets/JetsHists/$(vardir)/$(run)_iCent$(cent)_hists.root ./rootFiles/Trees/$(sigdir)/$(vardir)/$(run)_iCent$(cent).root
Error = run/errors/$(minjpt)GeVJets_JetsHists_$(run)_iCent$(cent)_$(vardir).err
Log = /tmp/jeff/logs/$(minjpt)GeVJets_JetsHists_$(run)_iCent$(cent)_$(vardir).log
Output = run/outputs/$(minjpt)GeVJets_JetsHists_$(run)_iCent$(cent)_$(vardir).out
Queue run in (312796 312837 312937 312945 312968 314199)


# p+Pb mixed
Arguments = $(minjpt) $(maxjpt) pPb16_5TeV $(minjpt)GeVJets_mixed_data16 ./rootFiles/Histograms/$(minjpt)GeVJets/MixedHists/$(vardir)/$(run)_iCent$(cent)_hists.root ./rootFiles/Trees/$(sigdir)/$(vardir)/$(run)_iCent$(cent).root ./rootFiles/Trees/$(mixdir)/$(vardir)/$(run)_iCent$(cent).root
Error = run/errors/$(minjpt)GeVJets_MixedHists_$(run)_iCent$(cent)_$(vardir).err
Log = /tmp/jeff/logs/$(minjpt)GeVJets_MixedHists_$(run)_iCent$(cent)_$(vardir).log
Output = run/outputs/$(minjpt)GeVJets_MixedHists_$(run)_iCent$(cent)_$(vardir).out
Queue run in (312796 312837 312937 312945 312968 314199)
